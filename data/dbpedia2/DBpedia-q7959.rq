  PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#> PREFIX dbpedia-owl: <http://dbpedia.org/ontology/> PREFIX dbpprop: <http://dbpedia.org/property/> PREFIX foaf: <http://xmlns.com/foaf/0.1/> SELECT ?name ?popularity ?abstract ?wikipedia ?img WHERE  { <http://dbpedia.org/resource/Funk_carioca> rdfs:label ?name ;           rdfs:comment ?abstract ;           foaf:page ?wikipedia .    OPTIONAL { <http://dbpedia.org/resource/Funk_carioca> dbpprop:popularity ?popularity . }    OPTIONAL { <http://dbpedia.org/resource/Funk_carioca> foaf:depiction ?img . }    FILTER ( langMatches(lang(?name), 'EN') &&             langMatches(lang(?abstract), 'EN') ) } 