PREFIX  :     <http://dbpedia.org/resource/> PREFIX  dc:   <http://purl.org/dc/elements/1.1/> PREFIX  dbpedia-owl: <http://dbpedia.org/ontology/> PREFIX  geo:  <http://www.w3.org/2003/01/geo/wgs84_pos#> PREFIX  foaf: <http://xmlns.com/foaf/0.1/> PREFIX  dbpedia-prop: <http://dbpedia.org/property/> PREFIX  yago: <http://dbpedia.org/class/yago/> PREFIX  rdfs: <http://www.w3.org/2000/01/rdf-schema#> PREFIX  umbel-sc: <http://umbel.org/umbel/sc/> PREFIX  dbpedia: <http://dbpedia.org/> PREFIX  xsd:  <http://www.w3.org/2001/XMLSchema#> PREFIX  owl:  <http://www.w3.org/2002/07/owl#> PREFIX  rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> PREFIX  skos: <http://www.w3.org/2004/02/skos/core#>  SELECT DISTINCT  ?treatment ?sameAsUri ?linkUrl WHERE   {   { :abdominoplasty                   dbpedia-prop:redirect  ?treatment .       }     UNION       { :Abdominoplasty                   dbpedia-prop:redirect  ?treatment .       }     UNION       { ?treatment  rdfs:label  'abdominoplasty'@en .}     UNION       { ?treatment  dbpedia-prop:name  'abdominoplasty'@en .}     UNION       { ?treatment  foaf:name  'abdominoplasty' .}     UNION       { ?treatment  rdfs:label  'Abdominoplasty'@en .}     UNION       { ?treatment  foaf:name  'Abdominoplasty' .}       { ?treatment  rdf:type  ?treatmentType .         FILTER regex(?treatmentType, 'http://dbpedia.org/class/yago/.*((((MedicalTreatment)|(Procedure)|(Surgery)|(Therapy)|(Prosthetic)).*)|(ics))', 'i')       }     UNION       { ?treatment  skos:subject  ?subject .         FILTER regex(?subject, 'http://dbpedia.org/resource/Category:.*((((Medical)|(surgery)|(procedure)|(medicine)|(therapy)|(acupuncture)|(treatment)|(Medical_terms)|(prosthetic)|(health)).*)|((ysis)|(logy)|(ication)))', 'i')       }     UNION       { ?treatment  dbpedia-prop:wikiPageUsesTemplate  ?template .         FILTER regex(?template, 'http://dbpedia.org/resource/Template:((infobox_intervention))', 'i')       }     OPTIONAL       {   { ?treatment  owl:sameAs  ?sameAsUri .}         UNION           { ?treatment  foaf:page  ?linkUrl .}       }   } 