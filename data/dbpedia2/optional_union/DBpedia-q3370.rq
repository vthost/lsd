nd  dbpedia-prop:wikiPageUsesTemplate  ?template .         FILTER regex(?template, 'http://dbpedia.org/resource/Template:((infobox_disease)|(diseasedisorder_infobox))', 'i')       }     OPTIONAL       {   { ?cond  owl:sameAs  ?sameAsUri .}         UNION           { ?cond  foaf:page  ?linkUrl .}       }   } 