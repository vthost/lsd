PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#> PREFIX dbpedia-owl: <http://dbpedia.org/ontology/> PREFIX owl: <http://www.w3.org/2002/07/owl#> PREFIX foaf: <http://xmlns.com/foaf/0.1/> PREFIX dbpprop: <http://dbpedia.org/property/> PREFIX mo: <http://purl.org/ontology/mo/> PREFIX dc: <http://purl.org/dc/elements/1.1/> PREFIX tags: <http://www.holygoat.co.uk/owl/redwood/0.1/tags/>SELECT * WHERE { { ?x rdf:type dbpedia-owl:Band } UNION { ?x rdf:type dbpedia-owl:MusicalArtist } ?x rdfs:label 'Alain Johannes'@en . ?x dbpedia-owl:abstract ?biography . FILTER langMatches(lang(?biography), 'en') ?wiki foaf:primaryTopic ?x . OPTIONAL { ?x dbpedia-owl:thumbnail ?thumbnail } OPTIONAL { ?x dbpedia-owl:activeYearsStartYear ?start } OPTIONAL { ?x dbpedia-owl:activeYearsEndYear ?end } OPTIONAL { ?x dbpprop:origin ?hometown } OPTIONAL { ?x dbpprop:birthPlace ?birthPlace } }