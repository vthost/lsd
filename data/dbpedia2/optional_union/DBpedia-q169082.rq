PREFIX  :     <http://dbpedia.org/resource/> PREFIX  dc:   <http://purl.org/dc/elements/1.1/> PREFIX  dbpedia-owl: <http://dbpedia.org/ontology/> PREFIX  geo:  <http://www.w3.org/2003/01/geo/wgs84_pos#> PREFIX  foaf: <http://xmlns.com/foaf/0.1/> PREFIX  dbpedia-prop: <http://dbpedia.org/property/> PREFIX  yago: <http://dbpedia.org/class/yago/> PREFIX  rdfs: <http://www.w3.org/2000/01/rdf-schema#> PREFIX  umbel-sc: <http://umbel.org/umbel/sc/> PREFIX  dbpedia: <http://dbpedia.org/> PREFIX  xsd:  <http://www.w3.org/2001/XMLSchema#> PREFIX  owl:  <http://www.w3.org/2002/07/owl#> PREFIX  rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> PREFIX  skos: <http://www.w3.org/2004/02/skos/core#>  SELECT DISTINCT  ?event ?sameAsUri ?linkUrl ?founded WHERE   {   { :Wimbledon  dbpedia-prop:redirect  ?event .}     UNION       { ?event  rdfs:label  'Wimbledon'@en .}     UNION       { ?event  dbpedia-prop:name  'Wimbledon'@en .}     UNION       { ?event  foaf:name  'Wimbledon' .}     UNION       { ?event  rdfs:label  'Wimbledon'@en .}     UNION       { ?event  rdfs:label  'The Wimbledon'@en .}     UNION       { ?event  foaf:name  'The Wimbledon' .}     UNION       { ?event  foaf:name  'Wimbledon' .}       { ?event  rdf:type  dbpedia-owl:SportsEvent .}     UNION       { ?event  rdf:type  ?eventType .         FILTER regex(?eventType, 'http://dbpedia.org/class/yago/.*((GolfTours)|(Tournament)|(PGATour)|(SportsFestival)|(Competition)|(Championship)|(SportsEvent)|(Racing)|(Marathon)).*', 'i')       }     UNION       { ?event  skos:subject  ?subject .         FILTER regex(?subject, 'http://dbpedia.org/resource/Category:.*((games)|(golf_tours)|(grand_prix)|(tournaments)|(pga_tour)|(competitions)|(sporting_events)|(championship)|(racing)|(marathon)).*', 'i')       }     UNION       { ?event  dbpedia-prop:wikiPageUsesTemplate  ?template .         FILTER regex(?template, 'http://dbpedia.org/resource/Template:.*((tournament)|(competition)|(race)).*', 'i')       }     OPTIONAL       {   { ?event  foaf:page  ?linkUrl .}         UNION           { ?event  owl:sameAs  ?sameAsUri .}       }     OPTIONAL       { ?event  dbpedia-prop:founded  ?founded .}   } 