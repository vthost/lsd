PREFIX owl: <http://www.w3.org/2002/07/owl#> PREFIX xsd: <http://www.w3.org/2001/XMLSchema#> PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#> PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> PREFIX foaf: <http://xmlns.com/foaf/0.1/> PREFIX dc: <http://purl.org/dc/elements/1.1/> PREFIX : <http://dbpedia.org/resource/> PREFIX dbpedia2: <http://dbpedia.org/property/> PREFIX dbpedia: <http://dbpedia.org/> PREFIX skos: <http://www.w3.org/2004/02/skos/core#> PREFIX dbpedia-owl: <http://dbpedia.org/ontology/> PREFIX dbpprop:	<http://dbpedia.org/property/> PREFIX yago: <http://dbpedia.org/class/yago/>  SELECT DISTINCT ?actor1 ?actor2 WHERE { ?actor1 rdf:type dbpedia-owl:Actor. ?actor2 rdf:type dbpedia-owl:Actor. { ?actor1 dbpedia-owl:partner ?actor2. } UNION { ?actor1 dbpedia-owl:spouse ?actor2. } ?pelicula1 ?starring ?actor1. ?pelicula1 ?starring ?actor2. ?pelicula2 ?starring ?actor1. ?pelicula2 ?starring ?actor2. ?pelicula3 dbpedia-owl:releaseDate ?releaseDate. ?pelicula3 dbpedia-owl:director ?director. ?director rdf:type yago:AmericanFilmDirectors. FILTER(?pelicula1 != ?pelicula2). FILTER(?actor1 != ?actor2). FILTER(?releaseDate > '1990-01-01'^^xsd:date). FILTER(?pelicula3 in (?pelicula2, ?pelicula1)). FILTER(?starring in (dbpprop:starring, dbpedia-owl:starring)). ?actor1 rdfs:label ?nameActor1. ?actor2 rdfs:label ?nameActor2. FILTER(regex(?nameActor1,'b','i') && regex(?nameActor2,'b','i')). } ORDER BY ?actor1