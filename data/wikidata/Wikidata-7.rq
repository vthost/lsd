PREFIX hint: <http://www.bigdata.com/queryHints#> PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#> PREFIX p: <http://www.wikidata.org/prop/> PREFIX wikibase: <http://wikiba.se/ontology#> PREFIX wdt: <http://www.wikidata.org/prop/direct/>  SELECT ?propUrl ?propLabel ?valUrl ?valLabel ?picture WHERE { 	hint:Query hint:optimizer 'None' . 	{	BIND(entity:Q42 AS ?valUrl) . 		BIND("N/A" AS ?propUrl ) . 		BIND("identity"@en AS ?propLabel ) . 	} 	UNION 	{	entity:Q42 ?propUrl ?valUrl . 		?property ?ref ?propUrl . 		?property rdf:type wikibase:Property . 		?property rdfs:label ?propLabel 	} 	   	?valUrl rdfs:label ?valLabel 	FILTER (LANG(?valLabel) = 'en') . 	OPTIONAL{ ?valUrl wdt:P18 ?picture .} 	FILTER (lang(?propLabel) = 'en' ) } ORDER BY ?propUrl ?valUrl LIMIT 200 