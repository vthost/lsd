 PREFIX xsd: <http://www.w3.org/2001/XMLSchema#> PREFIX wdt: <http://www.wikidata.org/prop/direct/>   SELECT ?person (MAX(?dateC) AS ?dateC_) WHERE { 	 	{ 		SELECT ?person (COUNT(?date) AS ?dateC) WHERE { 			?person wdt:P569 ?date . 			FILTER (?date > "1940-01-01T00:00:00Z"^^xsd:dateTime) . 		} 		GROUP BY ?person 		ORDER BY DESC (?dateC) 		LIMIT 100 	} UNION { 		SELECT ?person (COUNT(?date) AS ?dateC) WHERE  { 			?person wdt:P569 ?date . 			FILTER (?date <= "1965-01-01T00:00:00Z"^^xsd:dateTime) . 		} 		GROUP BY ?person 		ORDER BY DESC (?dateC) 		LIMIT 100 	} }  GROUP BY ?person ORDER BY DESC (?dateC) LIMIT 100 