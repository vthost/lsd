PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#> PREFIX p: <http://www.wikidata.org/prop/> PREFIX wikibase: <http://wikiba.se/ontology#> PREFIX wdt: <http://www.wikidata.org/prop/direct/> PREFIX wd: <http://www.wikidata.org/entity/>  SELECT ?s ?desc ?linkcount WHERE {   ?s wdt:P31 wd:Q37484 .   ?s wikibase:sitelinks ?linkcount .   OPTIONAL {      ?s rdfs:label ?desc filter (lang(?desc) = "en").    }  } GROUP BY ?s ?desc ?linkcount ORDER BY DESC(?linkcount) 