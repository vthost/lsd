PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#> PREFIX p: <http://www.wikidata.org/prop/> PREFIX wdt: <http://www.wikidata.org/prop/direct/> PREFIX wd: <http://www.wikidata.org/entity/>  SELECT DISTINCT * WHERE { 	?item 	wdt:P31 wd:Q8513 ; 		rdfs:label ?name . 	OPTIONAL {	?item  wdt:P275 ?licenseItem . 			?licenseItem rdfs:label ?license . 			FILTER (LANG(?license) = "en")} 	FILTER (LANG(?name) = "en")	 }  