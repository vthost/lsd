 PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#> PREFIX wdt: <http://www.wikidata.org/prop/direct/> PREFIX wikibase: <http://wikiba.se/ontology#> PREFIX bd: <http://www.bigdata.com/rdf#>   SELECT ?occup ?occupLabel ?count WHERE { 	{ 		SELECT ?occup (COUNT(?person) as ?count) 		WHERE 		{ 			?person wdt:P106 ?occup 		} 		GROUP BY ?occup 		ORDER BY DESC(?count) 		LIMIT 1000 	}   	OPTIONAL {?occup rdfs:label ?label1 	filter(lang(?label1) = "lv")} 	FILTER(!BOUND(?label1)) } ORDER BY DESC(?count) LIMIT 50 