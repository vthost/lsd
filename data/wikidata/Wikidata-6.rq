PREFIX xsd: <http://www.w3.org/2001/XMLSchema#> PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#> PREFIX p: <http://www.wikidata.org/prop/> PREFIX wikibase: <http://wikiba.se/ontology#> PREFIX wdt: <http://www.wikidata.org/prop/direct/> PREFIX wd: <http://www.wikidata.org/entity/>  SELECT ?s ?desc ?linkcount WHERE {   ?s wdt:P31 wd:Q5 ;         wdt:P21 wd:Q6581072 ;        wdt:P569 ?born .   FILTER (?born >= "1921-01-01T00:00:00Z"^^xsd:dateTime) .   ?s wikibase:sitelinks ?linkcount .   MINUS {     ?s wdt:P18 []     } ?s rdfs:label ?desc FILTER(lang(?desc)="en"). } GROUP BY ?s ?desc ?linkcount ORDER BY DESC(?linkcount) LIMIT 50  