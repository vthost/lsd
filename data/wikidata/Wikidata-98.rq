 PREFIX wdt: <http://www.wikidata.org/prop/direct/> PREFIX wd: <http://www.wikidata.org/entity/>   SELECT ?element (SAMPLE(?symbol) AS ?symbol_) (SAMPLE(?protons) AS ?protons_) (MIN(?neutrons) AS ?minNeutrons) (MAX(?neutrons) AS ?maxNeutrons) WHERE {   ?element wdt:P31 wd:Q11344;            wdt:P1086 ?protons;            wdt:P246 ?symbol.   ?isotope wdt:P279 ?element;            wdt:P1148 ?neutrons. } GROUP BY ?element ORDER BY ?protons 