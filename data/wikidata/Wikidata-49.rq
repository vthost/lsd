 PREFIX wikibase: <http://wikiba.se/ontology#> PREFIX wdt: <http://www.wikidata.org/prop/direct/> PREFIX wd: <http://www.wikidata.org/entity/> PREFIX bd: <http://www.bigdata.com/rdf#>   SELECT 	?country ?countryLabel 	?wikiPersons 	?population 	(ROUND(?wikiPersons/?population*1000000) AS ?wikiPersonsPerM) WHERE { 	{ SELECT ?country (count(*) as ?wikiPersons) WHERE { 		{SELECT DISTINCT ?person ?country WHERE { 			?country wdt:P31 wd:Q185441 . 			 			?person wdt:P31 wd:Q5 . 			?person wdt:P27 ?country . 			FILTER NOT EXISTS{ ?person wdt:P570 ?date }  		} } 	} GROUP BY ?country  } 	?country wdt:P1082 ?population  } ORDER BY DESC(?wikiPersonsPerM) 