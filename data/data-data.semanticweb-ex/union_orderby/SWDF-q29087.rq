PREFIX swrc: <http://swrc.ontoware.org/ontology#> PREFIX dc:   <http://purl.org/dc/elements/1.1/> PREFIX dct:   <http://purl.org/dc/terms/> PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#> PREFIX swc:  <http://data.semanticweb.org/ns/swc/ontology#> SELECT DISTINCT ?paper_uri ?paper_name ?event_uri ?event_type ?event_name ?event_acronym WHERE { 	GRAPH ?graph { 		?paper_uri a swrc:InProceedings . 		{ ?paper_uri dc:title ?paper_name } UNION { ?paper_uri dct:title ?paper_name } . 	} 	?event_uri a ?event_type . 	?event_uri swc:completeGraph ?graph . 	?event_uri rdfs:label ?event_name . 	?event_uri swc:hasAcronym ?event_acronym . } ORDER BY ?event_type ?event_acronym ?paper_name 