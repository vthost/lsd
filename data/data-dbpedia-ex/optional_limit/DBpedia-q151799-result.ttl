  rs:solution        [ rs:binding  [ rs:value     <http://dbpedia.org/resource/Wolfgang_M%C3%BCller> ;
                                     rs:variable  "musician"
                                   ] ;
            rs:binding  [ rs:value     "Wolfgang Müller ist ein deutscher Künstler, Musiker und Autor. Heute lebt er abwechselnd in Berlin und Reykjavík."@de ;
                          rs:variable  "description_de"
                        ] ;
            rs:binding  [ rs:value     "Wolfgang Müller (born 24 October 1957 in Wolfsburg) is a German artist, musician and writer, based in Berlin, Germany and Reykjavík, Iceland. Müller is the founder of the Berlin based multi-media performance art group Die Tödliche Doris ('The Deadly Doris'). The group was active for seven years, between 1980 and 1987. Müller published the book Geniale Dilletanten in 1982, naming a musical genre encompassing Die Tödliche Doris, Einstürzende Neubauten, Sprung Aus Den Wolken and many others."@en ;
                          rs:variable  "description_en"
                        ] ;
            rs:binding  [ rs:value     "Wolfgang Müller"@en ;
                          rs:variable  "name"
                        ] ] ;
  rs:solution        [ rs:binding  [ rs:value     <http://dbpedia.org/resource/Farin_Urlaub> ;
                                     rs:variable  "musician"
                                   ] ;
            rs:binding  [ rs:value     "Farin Urlaub ist ein deutscher Rockmusiker, Sänger und Gitarrist. Bekannt wurde Urlaub als Gründungsmitglied der Rockband „Die Ärzte“, außerdem ist er seit 2001 als Solokünstler aktiv und Kopf des „Farin Urlaub Racing Teams“."@de ;
                          rs:variable  "description_de"
                        ] ;
            rs:binding  [ rs:value     "Jan Ulrich Max Vetter, better known as Farin Urlaub (like German \"Fahr in Urlaub!\", after his love of travelling) was born on October 27, 1963 in what was then West Berlin, Germany . He is best known as the guitarist/vocalist for the German punk rock band Die Ärzte. He has also been a solo artist since 2001, touring with his band, the \"Farin Urlaub Racing Team\"."@en ;
                          rs:variable  "description_en"
                        ] ;
            rs:binding  [ rs:value     "Farin Urlaub"@de ;
                          rs:variable  "name"
                        ] ] ;
  rs:solution        [ rs:binding  [ rs:value     <http://dbpedia.org/resource/Heinz-Klaus_Metzger> ;
                                     rs:variable  "musician"
                                   ] ;
            rs:binding  [ rs:value     "Heinz-Klaus Metzger war ein deutscher Musiktheoretiker und Musikkritiker. Er galt als einer der bedeutendsten Theoretiker der Neuen Musik nach 1945. Zusammen mit Rainer Riehn war er Begründer und Herausgeber der musikwissenschaftlichen Reihen Musik-Konzepte (bis 2003) und querstand. musikalische konzepte."@de ;
                          rs:variable  "description_de"
                        ] ;
            rs:binding  [ rs:value     "Heinz-Klaus Metzger (Born February 6, 1932 in Konstanz; died October 25, 2009 in Berlin) was a German music critic and theorist. Metzger studied piano under Carl Seemann in Freiburg and composition under Max Deutsch in Paris. Later, attending a summer course for new music in Darmstadt, he met Theodor W. Adorno, Edgard Varèse, Karlheinz Stockhausen and Luigi Nono. Here he found his role as a notable theoretician and proponent of Serialism in musical theory."@en ;
                          rs:variable  "description_en"
                        ] ;
            rs:binding  [ rs:value     "Heinz-Klaus Metzger"@de ;
                          rs:variable  "name"
                        ] ] ;
