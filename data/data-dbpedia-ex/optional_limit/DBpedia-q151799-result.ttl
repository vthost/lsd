  rs:solution        [ rs:binding  [ rs:value     <http://dbpedia.org/resource/Micki_Meuser> ;
                                     rs:variable  "musician"
                                   ] ;
            rs:binding  [ rs:value     "Hans Georg \"Micki\" Meuser ist ein deutscher Bassist und Musikproduzent. Mit der Jazzrock Gruppe Out hatte Meuser erste überregionale Erfolge. Nachdem Meuser als Bassist in Köln und Hamburg in verschiedenen Jazzbands aktiv war, veröffentlichte er erstmals 1978 zusammen mit Charlie Mariano ein Album (\"October\"). In den folgenden Jahren konzentrierte er sich eher auf Musikproduktionen und war als Produzent in Deutschland überaus erfolgreich."@de ;
                          rs:variable  "description_de"
                        ] ;
            rs:binding  [ rs:value     "Micki Meuser a.k. a Mickey Meuser (real name Hans-Georg Meuser) from Alsdorf near Aachen, Germany. Bass player, studio musician and music producer for Die Ärzte, Ideal, Ina Deter, Lemonbabies and other german bands. In 1974 he played the electric bass guitar in diverse formations like and the Alsdorf Big Band. In the early 1980s he formed the Nervösen Deutschen and brought Australian lead singer Grant Stevens from London to Germany."@en ;
                          rs:variable  "description_en"
                        ] ;
            rs:binding  [ rs:value     "Micki Meuser"@de ;
                          rs:variable  "name"
                        ] ] ;
