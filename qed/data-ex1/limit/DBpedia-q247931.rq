PREFIX geo:<http://www.w3.org/2003/01/geo/wgs84_pos#>  PREFIX xsd:<http://www.w3.org/2001/XMLSchema#>   SELECT ?l ?t WHERE {   ?l geo:lat ?lat; geo:long ?lng; a ?t.   FILTER((?lat - xsd:float('50.936053'))*(?lat - xsd:float('50.936053'))+(?lng - xsd:float('-1.395864'))*(?lng - xsd:float('-1.395864'))<xsd:float('5'))  } limit 10